{
    "name": "UpBeat",
    "description": "self hosted server uptime monitor",
    "keywords": ["MERN", "NestJS", "Node", "MongoDB", "GraphQL"],
    "repository": "https://github.com/soorajshankar/up-beat",
    "success_url": "/",
    "formation": {
        "web": {
            "quantity": 1,
            "size": "free"
        }
    },
    "addons": [
        {
            "plan": "mongolab:shared-single-small",
            "as": "MONGO"
        }
    ],
    "env": {
        "SECRET_TOKEN": {
            "description": "A secret key for verifying the integrity of signed cookies.",
            "generator": "secret"
        }
    }
}
